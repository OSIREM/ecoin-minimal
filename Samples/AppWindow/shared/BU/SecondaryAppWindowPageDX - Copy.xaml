                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0"/>
                                <GradientStop Color="#FFA1B2B1" Offset="1"/>
                                <GradientStop Color="#FF85D8D8" Offset="0.162"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <TextBlock x:Name="titletoolBlock" TextWrapping="Wrap" Text="-Visual Mining Tools" FontSize="24" HorizontalAlignment="Left" Foreground="#FFE68612" Margin="4,4,0,4" Width="531" d:LayoutOverrides="TopPosition, BottomPosition"/>
