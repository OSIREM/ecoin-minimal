                    <TextBlock x:Name="playbackviewBlock" TextWrapping="Wrap" Text="  Frame Controls" FontSize="30" HorizontalAlignment="Left" Foreground="#FF9B5A0B" Margin="6,-2,0,23" Width="264" Grid.Row="1" d:LayoutOverrides="LeftMargin, RightMargin, TopPosition, BottomPosition"/>
                    <Grid x:Name="PlaybackGrid" BorderBrush="#FF807536" BorderThickness="6" Grid.Column="0" Grid.Row="2" Margin="16,-22,14,4" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
                        <Grid.Background>
                            <ImageBrush Stretch="Fill" ImageSource="Assets/images.jpg"/>
                        </Grid.Background>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="130*"/>
                            <ColumnDefinition Width="19*"/>
                            <ColumnDefinition Width="111*"/>
                            <ColumnDefinition Width="131*"/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="previousfrmButton" Content="" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="OnClickBack" Margin="14,15,1,17" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
                            <Button.Background>
                                <ImageBrush Stretch="Fill" ImageSource="Assets/skip_button_left.png"/>
                            </Button.Background>
                        </Button>
                        <Button x:Name="stopfrmButton" Content="" Grid.Column="2" HorizontalAlignment="Stretch" Margin="-3.333,13,0,16" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" VerticalAlignment="Stretch" Click="onClickLookAt">
