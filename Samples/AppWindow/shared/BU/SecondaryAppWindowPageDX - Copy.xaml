                    <RadioButton x:Name="codeButton2" Content="Operator (Assign)" VerticalAlignment="Bottom" Grid.ColumnSpan="2" Grid.Column="5" Height="43" Width="270" Grid.Row="3" Canvas.ZIndex="3" Foreground="#FF36E4E4" Margin="-5.667,0,-31,-12" Click="OnClickRadio2"/>
                    <RadioButton x:Name="codeButton3" Content="Function (Math)" Margin="-5.667,0,-2,5" VerticalAlignment="Bottom" Grid.ColumnSpan="2" Grid.Column="5" Height="42" Grid.Row="4" Canvas.ZIndex="3" Foreground="Red" Width="241" BorderBrush="Red" Click="OnClickRadio3"/>
                    <ComboBox x:Name="codeType" HorizontalAlignment="Stretch" Height="54" Margin="14,0,-27,2" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="7" Canvas.ZIndex="3" Grid.ColumnSpan="2" d:LayoutOverrides="LeftPosition, RightPosition" SelectionChanged="OnSelectChangeType"/>
                    <TextBlock x:Name="codecodeTitle" Margin="16,-7,-25,10" TextWrapping="Wrap" VerticalAlignment="Bottom" Foreground="#FF245D4D" Grid.Column="1" Height="57" Grid.ColumnSpan="4" RenderTransformOrigin="0.5,0.5" Grid.Row="5" Canvas.ZIndex="3" Text="Command in normal C or BASIC Coding Style" d:LayoutOverrides="LeftPosition, RightPosition"/>
                    <TextBlock x:Name="codeTypeTitle" Margin="24,0,20,-4" TextWrapping="Wrap" VerticalAlignment="Bottom" Foreground="White" Text="Type:" Grid.Column="1" Height="22" Grid.Row="6" Canvas.ZIndex="3" Grid.ColumnSpan="2" d:LayoutOverrides="VerticalAlignment, LeftPosition, RightPosition"/>
                    <TextBox x:Name="codeName" Margin="-22,6,19,4" TextWrapping="Wrap" Text="" Grid.ColumnSpan="3" Grid.Column="4" Grid.Row="7" Canvas.ZIndex="3" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" TextChanged="OnLostFocusName"/>
                    <TextBlock x:Name="codeNameTitle" Margin="-26,0,-21,0" TextWrapping="Wrap" VerticalAlignment="Bottom" Foreground="White" Text="Name:" Grid.Column="4" d:LayoutOverrides="VerticalAlignment, LeftPosition, RightPosition" Height="18" Grid.Row="6" Canvas.ZIndex="3"/>
                    <Button x:Name="codeVar1" Content="Var1" Margin="-24,-5,8,25" VerticalAlignment="Bottom" BorderBrush="#FF2F9509" BorderThickness="3" Foreground="White" Grid.Column="2" Height="40" Grid.Row="6" Canvas.ZIndex="3" Click="OnClickVar1" HorizontalAlignment="Stretch" d:LayoutOverrides="LeftPosition, RightPosition"/>
                    <Button x:Name="codeOperator" HorizontalAlignment="Stretch" Margin="8,-4,8,19" Content="=" VerticalAlignment="Bottom" Foreground="White" FontSize="14.667" Height="45" Grid.Row="6" RenderTransformOrigin="0.5,0.5" Canvas.ZIndex="3" Grid.Column="3" BorderBrush="#FF35E0F1" BorderThickness="4" Click="OnClickOperator" d:LayoutOverrides="LeftPosition, RightPosition"/>
                    <TextBox x:Name="codeInput" Margin="-10,6,-17,4" TextWrapping="Wrap" Text="0.0" Grid.Column="7" Canvas.ZIndex="3" RenderTransformOrigin="0.5,0.5" Grid.Row="7" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Grid.ColumnSpan="2" TextChanged="onLostFocusInput"/>
                    <TextBlock x:Name="codeTextValue" HorizontalAlignment="Left" Margin="-12,40,-9,-2" TextWrapping="Wrap" Foreground="White" Text="Value:" Grid.Column="7" Width="77" Grid.Row="6" Height="22" VerticalAlignment="Top" Canvas.ZIndex="3"/>
                    <TextBlock x:Name="codeResult" Margin="10,-5,13,19" TextWrapping="Wrap" Text="0.0" VerticalAlignment="Bottom" FontSize="29.333" Foreground="White" Grid.Column="4" Height="46" Grid.Row="6" Canvas.ZIndex="4" Grid.ColumnSpan="3" d:LayoutOverrides="LeftPosition, RightPosition"/>
                    <Button x:Name="codeVar2" Content="Var2" Margin="11,-1,10,21" VerticalAlignment="Stretch" BorderBrush="#FF2F9509" BorderThickness="3" Foreground="White" Grid.Column="4" Grid.Row="6" Canvas.ZIndex="3" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Grid.ColumnSpan="2" Click="OnClickVar2" HorizontalAlignment="Stretch"/>
                    <TextBlock x:Name="codeargBlock" Margin="8,1,11,0" TextWrapping="Wrap" Text="(" VerticalAlignment="Top" FontSize="29.333" Foreground="#FFF70000" Grid.Column="6" Height="36" Grid.Row="6" Canvas.ZIndex="3" d:LayoutOverrides="HorizontalAlignment, LeftPosition, RightPosition"/>
                    <Button x:Name="codeArg1" Content="Arg1" Margin="-3,2,1,21" VerticalAlignment="Stretch" BorderBrush="#FFAA0A0A" BorderThickness="3" Foreground="White" Grid.Column="7" Grid.Row="6" Canvas.ZIndex="3" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickArg1" HorizontalAlignment="Stretch"/>
