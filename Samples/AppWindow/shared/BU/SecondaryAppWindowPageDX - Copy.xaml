                    <Button x:Name="Tab9" Grid.Row="1" Content="Tab9" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="9" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab9"/>
                    <Button x:Name="Example1" Grid.Row="3" Content="OSI Commander" Margin="0,0,3,4" VerticalAlignment="Stretch" Grid.Column="10" HorizontalAlignment="Stretch" BorderThickness="0,3" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Foreground="Black" Click="OnClickOSICommander">
