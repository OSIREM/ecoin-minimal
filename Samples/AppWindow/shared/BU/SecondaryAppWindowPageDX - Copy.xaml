                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFF0680A" Offset="0"/>
                                <GradientStop Color="White" Offset="0.302"/>
                                <GradientStop Color="#FFC9270D" Offset="0.976"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Button Content="Command Generator" Margin="12,0,2,-4" VerticalAlignment="Bottom" Grid.ColumnSpan="5" HorizontalAlignment="Stretch" Grid.Row="8" d:LayoutOverrides="LeftPosition, RightPosition" Grid.Column="1" BorderThickness="4,4,4,6" Background="#3348C124" BorderBrush="#FF00B200" Foreground="#FF8EFFFF" Click="OnClickCommandGenerator" Height="52"/>
                    <Rectangle x:Name="codeGenerator" Margin="-1,-12,-3,-13" Stroke="Black" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Grid.ColumnSpan="9" Grid.Column="1" RenderTransformOrigin="0.5,0.5" Canvas.ZIndex="2" Grid.Row="2" Grid.RowSpan="6">
                        <Rectangle.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFE28A20" Offset="0"/>
                                <GradientStop Color="White" Offset="1"/>
                                <GradientStop Color="#FF232323" Offset="0.135"/>
                                <GradientStop Color="#FFE8E7DF" Offset="0.41"/>
                                <GradientStop Color="#FF753BBB" Offset="0.637"/>
                                <GradientStop Color="#FFD5D5D5" Offset="0.837"/>
                                <GradientStop Color="#FFAFAEA8" Offset="0.139"/>
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                    <TextBlock x:Name="codeTitle" Margin="8,8,-27,0" TextWrapping="Wrap" d:LayoutOverrides="LeftPosition, RightPosition" FontFamily="Prosto One" RenderTransformOrigin="0.5,0.5" Text="ESL Script Command Generator" Canvas.ZIndex="3" Grid.ColumnSpan="9" Grid.Column="1" Grid.Row="2" Height="38" VerticalAlignment="Top" FontSize="24" Foreground="#FFF1D760"/>
