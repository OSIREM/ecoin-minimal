                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0"/>
                                <GradientStop Color="White" Offset="1"/>
                                <GradientStop Color="#FF686868" Offset="0.762"/>
                                <GradientStop Color="#FF8DEA62" Offset="0.671"/>
                                <GradientStop Color="#FF500D87" Offset="0.238"/>
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                </Grid>
                <Grid x:Name="ViewGrid" BorderBrush="#FF807536" BorderThickness="2" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="60"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.3333*"/>
                        <ColumnDefinition Width="0.3333*"/>
                        <ColumnDefinition Width="0.3333*"/>
                    </Grid.ColumnDefinitions>
                    <Image Source="Assets\ecoin.png" Margin="9.333,24,0,-4" d:LayoutOverrides="TopPosition, BottomPosition" Grid.Column="1" HorizontalAlignment="Left" Width="152" Grid.RowSpan="2" Grid.Row="1"/>
