                    <Button x:Name="Tab1" Grid.Row="1" Content="Tab1" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="1" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab1"/>
                    <Button x:Name="Tab2" Grid.Row="1" Content="Tab2" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="2" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab2"/>
