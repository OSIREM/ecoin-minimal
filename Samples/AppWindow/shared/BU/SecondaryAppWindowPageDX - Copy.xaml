                                <GradientStop Color="White" Offset="0.997"/>
                                <GradientStop Color="White" Offset="0.003"/>
                                <GradientStop Color="#FF414141" Offset="0.259"/>
                                <GradientStop Color="#FF323232" Offset="0.723"/>
                            </LinearGradientBrush>
                        </Button.BorderBrush>
                    </Button>
                    <Rectangle Margin="-11,-8,0,2" Stroke="Black" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
                        <Rectangle.Fill>
