                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#59F5C319" Offset="1"/>
                    <GradientStop Color="#0CC2FF3E" Offset="0.163"/>
                    <GradientStop Color="#0CDDE2E6" Offset="0.618"/>
                    <GradientStop Color="#0CF5C319" Offset="0.833"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
