                        <TextBox.Foreground>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0.863"/>
                                <GradientStop Color="#FF3DF1D1" Offset="1"/>
                                <GradientStop Color="Black" Offset="0.204"/>
                                <GradientStop Color="White" Offset="0.116"/>
                                <GradientStop Color="#FF723EA6" Offset="0.046"/>
                                <GradientStop Color="#FFD7DA9B" Offset="0.747"/>
                                <GradientStop Color="#FF743241" Offset="0.378"/>
                            </LinearGradientBrush>
                        </TextBox.Foreground>
                    </TextBox>
                    <Button x:Name="OpenESLbutton" Content="Open" HorizontalAlignment="Left" Margin="2,0,0,2" VerticalAlignment="Stretch" Grid.Row="1" Width="93" d:LayoutOverrides="TopPosition, BottomPosition" Grid.Column="10"/>
