                    <Button x:Name="Tab5" Grid.Row="1" Content="Tab5" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="5" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab5"/>
                    <Button x:Name="Tab6" Grid.Row="1" Content="Tab6" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="6" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab6"/>
                    <Button x:Name="Tab7" Grid.Row="1" Content="Tab7" Margin="4,3,3,4" VerticalAlignment="Stretch" Grid.Column="7" HorizontalAlignment="Stretch" BorderThickness="0,3" Background="#33A7AC75" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Click="OnClickTab7"/>
