                    <Button x:Name="OptionsButton" Grid.Row="4" Content="OPTIONS" Margin="0,0,3,4" VerticalAlignment="Stretch" Grid.Column="10" HorizontalAlignment="Stretch" BorderThickness="0,3" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" Foreground="Black">
