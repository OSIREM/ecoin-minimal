                        </Rectangle.Fill>
                    </Rectangle>
                    <Button x:Name="executeButton" Content="Execute" Grid.Column="10" HorizontalAlignment="Stretch" Margin="4,2,6,4" Grid.Row="7" VerticalAlignment="Stretch" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" BorderBrush="#FF5B5A5A" Foreground="#FF252525" Canvas.ZIndex="-12" FontFamily="Segoe UI Black" Click="OnClickExecute">
